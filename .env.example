# Supabase environment variables template
# Copy this file to .env and fill in real values. Do not commit secrets.
# Helen app uses dedicated Supabase instance (separate from Persona app)

VITE_SUPABASE_URL=https://lvkbmxtxbwxjwzquzlws.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Existing keys used by the app
VITE_GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Clerk Authentication (Optional)
# Get your publishable key from https://clerk.com/
# Note: pk_test_* keys are for development only
# For production, use pk_live_* keys from your production instance
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Stripe + Supabase billing (Next.js + FastAPI)
# Frontend (Next.js)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Backend (shared)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Supabase (backend service role for webhook upserts)
# Helen app dedicated Supabase instance
SUPABASE_URL=https://lvkbmxtxbwxjwzquzlws.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_KEY=your_service_role_key_here
